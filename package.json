{
  "name": "classic-pairings",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "auth0-js": "^9.8.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cli": "^1.0.1",
    "codebuilder": "^1.0.0",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "glamor": "^2.20.40",
    "mongoose": "^5.3.4",
    "qrcode.react": "^0.8.0",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-axios": "^2.0.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.5"
  },
  "engines": {
    "node": "4.1.1"
  },
  "peerDependencies": {
    "apollo-client": "^2.3.8",
    "axios": "0.18.0",
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "prop-types": "15.6.2",
    "react": "0.14.x || 15.* || ^15.0.0 || ^16.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "A MERN application based on the idea of wine pairing with movies, food and meetups in the Central Florida region.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kathyboilek/Classic-Pairings.git"
  },

